<h1>Listing ubigeos</h1>

<table>
  <thead>
    <tr>
      <th>Name</th>
      <th>Level</th>
      <th>Code</th>
      <th>Status</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @ubigeos.each do |ubigeo| %>
      <tr>
        <td><%= ubigeo.name %></td>
        <td><%= ubigeo.level %></td>
        <td><%= ubigeo.code %></td>
        <td><%= ubigeo.status %></td>
        <td><%= link_to 'Edit', edit_ubigeo_path(ubigeo) %></td>
        
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Ubigeo', new_ubigeo_path %>

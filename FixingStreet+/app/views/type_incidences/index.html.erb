<h1>Listado de tipo de incidencias</h1>

<table>
  <thead>
    <tr>
      <th>Nombre</th>
      <th>Estado</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @type_incidences.each do |type_incidence| %>
      <tr>
        <td><%= type_incidence.name %></td>
        <td><%= type_incidence.status %></td>
        <td><%= link_to 'Editar', edit_type_incidence_path(type_incidence) %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'Crear', new_type_incidence_path %>

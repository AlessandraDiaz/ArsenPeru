<h1>Listing type_incidences</h1>

<table>
  <thead>
    <tr>
      <th>Name</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @type_incidences.each do |type_incidence| %>
      <tr>
        <td><%= type_incidence.name %></td>
        <td><%= link_to 'Show', type_incidence %></td>
        <td><%= link_to 'Edit', edit_type_incidence_path(type_incidence) %></td>
        <td><%= link_to 'Destroy', type_incidence, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Type incidence', new_type_incidence_path %>

<h1>Listing type_users</h1>

<table>
  <thead>
    <tr>
      <th>Name</th>
      <th>Status</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @type_users.each do |type_user| %>
      <tr>
        <td><%= type_user.name %></td>
        <td><%= type_user.status %></td>
        <td><%= link_to 'Edit', edit_type_user_path(type_user) %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Type user', new_type_user_path %>
